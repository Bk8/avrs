#!/bin/bash

rmmod=/sbin/rmmod

if test \! "x`type -t sudo`" = x; then
  sudo=sudo
fi

sync

for module in rtai_mbx rtai_sem rtai_fifos rtai_lxrt rtai_hal; do
	$sudo $rmmod $module
done

sync


